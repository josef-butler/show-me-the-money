{
  "name": "show-me-the-money",
  "version": "0.3.0",
  "description": "$how Me The Money",
  "repository": "https://github.com/dev-academy-challenges/show-me-the-money.git",
  "main": "server/index.js",
  "scripts": {
    "dev": "run-p watch:client watch:server",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon --watch server",
    "start": "node server",
    "test": "jest --noStackTrace --watchAll --color",
    "postinstall": "webpack",
    "h:deploy": "git push heroku master",
    "h:migrate": "heroku run npx knex migrate:latest",
    "h:rollback": "heroku run npx knex migrate:rollback",
    "h:seed": "heroku run npx knex seed:run"
  },
  "author": "EDA",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup-dotenv.js",
      "./tests/client/setup-enzyme.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "authenticare": "^0.4.7",
    "webpack": "^4.43.0",
    "bulma": "^0.9.0",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.2.2",
    "react": "^16.13.1",
    "react-charts": "^2.0.0-beta.7",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^9.0.2",
    "superagent": "^5.3.1"
  }
}
